//opening background
Titanium.UI.setBackgroundColor('black');

//Start Window
var Background = Ti.UI.createWindow({
	backgroundColor: "black"
});

//Start Main Title
var StartTitle = Ti.UI.createView({
	text: "Full Throttle Gaming",
	height: 70,
	top: 0,
	backgroundColor: "white"
});

Background.open();
Background.add(StartTitle);

